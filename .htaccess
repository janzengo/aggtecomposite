RewriteOptions inherit

RewriteEngine On


RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

# Custom
#remove php file extension-e.g. https://example.com/file.php will become https://example.com/file 
RewriteEngine on
RewriteRule ^(php)($|/) - [L]
RewriteCond %{THE_REQUEST} /([^.]+)\.php [NC]
RewriteRule ^ /%1 [NC,L,R]
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^ %{REQUEST_URI}.php [NC,L]
RewriteCond %{HTTP_HOST} ^mail\.aggtedeck\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.mail\.aggtedeck\.com$
RewriteRule ^/?$ "https\:\/\/webmail\.aggtedeck\.com\/" [R=301,L]
RewriteCond %{HTTP_HOST} ^cpanel\.aggtedeck\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.cpanel\.aggtedeck\.com$
RewriteRule ^/?$ "https\:\/\/my\.web\.z\.com\/" [R=301,L]

# BEGIN cPanel-generated php ini directives, do not edit
# Manual editing of this file may result in unexpected behavior.
# To make changes to this file, use the cPanel MultiPHP INI Editor (Home >> Software >> MultiPHP INI Editor)
